{
  "name": "doctel-mobile-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npx react-native run-android",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "buildApk": "npx react-native bundle --platform android --dev false --entry-file App.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && cd android && gradlew assembleRelease"
  },
  "dependencies": {
    "@expo/metro-config": "^0.19.11",
    "@react-native-community/cli": "^15.1.3",
    "expo": "~52.0.37",
    "expo-status-bar": "~2.0.1",
    "react": "18.3.1",
    "react-native": "0.76.7",
    "react-native-svg-transformer": "^1.5.0",
    "expo-system-ui": "~4.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "private": true
}
